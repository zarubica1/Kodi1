<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.program.adamswizard" name="[COLOR limegreen][B]Adam's[/B][/COLOR]Wizard" version="1.0" provider-name="adamsbuilds">
    <requires>
        <import addon="xbmc.python" version="2.1.0" />
        <import addon="script.module.requests" />
        <import addon="script.module.six" />
    </requires>
    
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>executable</provides>
    </extension>
    
    <extension point="xbmc.service" library="startup.py" start="startup" />
    
    <extension point="xbmc.addon.metadata">
        <summary lang="en">[COLOR limegreen][B]Adam's[/B][/COLOR]Wizard</summary>
        <description lang="en">Wizard offers many maintenance features, selective add-on data saving, and automated build installs.</description>
        <platform>all</platform>

    </extension>
</addon>
<addon id="repository.adamsbuilds" name="adamsbuilds" version="1.0" provider-name="adamsbuilds">
    <extension point="xbmc.addon.repository" name="adamsbuilds repository">
        <dir>
            <info compressed="false">https://raw.githubusercontent.com/adamsbuilds/repo/master/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/adamsbuilds/repo/master/zips/addons.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/adamsbuilds/repo/master/zips/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>adamsbuilds</summary>
        <description>adamsbuilds repository</description>
        <disclaimer></disclaimer>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
    </extension>
</addon>
</addons>
